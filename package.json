{
  "name": "template-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/nafkhanzam/template-backend.git",
  "author": "nafkhanzam <nafkhanalzamzami@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn modelgen && yarn types && yarn build:reflection && tsc --build tsconfig.build.json",
    "build:reflection": "cross-env NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true yarn ts-node src",
    "dev:default": "yarn modelgen && yarn types && cross-env yarn ts-node-dev src",
    "dev": "cross-env PORT=4000 yarn dev:default",
    "dev:test": "cross-env NODE_ENV=test PORT=9999 yarn dev:default",
    "generate": "prisma generate",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/src",
    "test": "cross-env NODE_ENV=test PORT=9999 jest --runInBand",
    "ts-node": "ts-node -r tsconfig-paths/register --transpile-only",
    "ts-node-dev": "ts-node-dev -r tsconfig-paths/register --transpile-only",
    "modelgen": "pal g",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "dbpush": "prisma db push --preview-feature",
    "types": "yarn ts-node generateExports",
    "seed": "yarn ts-node seeder"
  },
  "dependencies": {
    "@nafkhanzam/backend-utils": "^0.0.18",
    "@nafkhanzam/common-utils": "^0.0.15",
    "@prisma/client": "^2.16.1",
    "apollo-server-express": "^2.20.0",
    "express": "^4.17.1",
    "express-rescue": "^1.1.31",
    "graphql": "^15.5.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-upload": "^11.0.0",
    "ioredis": "^4.22.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.30.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@paljs/cli": "^2.12.2",
    "@paljs/nexus": "^2.12.2",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/graphql-upload": "^8.0.4",
    "@types/ioredis": "^4.19.3",
    "@types/jest": "^26.0.20",
    "cross-env": "^7.0.3",
    "export-generator": "^1.2.0",
    "faker": "^5.3.1",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "prisma": "^2.16.1",
    "prisma-dbml-generator": "^0.4.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.4"
  }
}
