{
  "name": "template-backend",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/nafkhanzam/template-backend.git",
  "author": "nafkhanzam <nafkhanalzamzami@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "yarn generate && yarn types && yarn gen-oa",
    "build": "yarn build:reflection && tsc --build tsconfig.build.json && cp ./src/context.ts ./build/src",
    "build:reflection": "cross-env NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true yarn ts-node src",
    "dev:default": "cross-env yarn ts-node-dev src",
    "dev": "cross-env PORT=4000 yarn dev:default",
    "generate": "yarn prisma generate",
    "start": "node -r ./tsconfig-bootstrap-paths.js build/src",
    "test": "cross-env NODE_ENV=test jest --runInBand --bail --verbose",
    "ts-node": "ts-node -r tsconfig-paths/register --transpile-only",
    "ts-node-dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --exit-child",
    "modelgen": "pal g",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "dbpush": "yarn prisma db push",
    "types": "yarn nz gen-all",
    "seed": "yarn ts-node seeder",
    "tc": "tsc --build tsconfig.watch.json",
    "gen-oa": "cross-env GENERATE_OPEN_API=true yarn ts-node src"
  },
  "dependencies": {
    "@nafkhanzam/backend-utils": "^0.0.47",
    "@nafkhanzam/common-utils": "^0.0.56",
    "@prisma/client": "^2.28.0",
    "apollo-server-express": "^3.1.1",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-rescue": "^1.1.31",
    "express-zod-api": "^2.0.0",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-upload": "^12.0.0",
    "lodash": "^4.17.21",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-generic-sdk": "^1.17.15",
    "@graphql-codegen/typescript-graphql-request": "^3.2.5",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@nafkhanzam/nz-cli": "^0.0.18",
    "@paljs/cli": "^3.7.1",
    "@paljs/nexus": "^3.6.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv-flow": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/fs-extra": "^9.0.12",
    "@types/graphql-upload": "^8.0.6",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "jest-html-reporters": "^2.1.6",
    "prisma": "^2.28.0",
    "prisma-dbml-generator": "^0.6.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
