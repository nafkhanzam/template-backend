{
  "gen-exports": [
    {
      "globs": ["src/common/**/*.ts"],
      "output": "src/common/index.ts"
    },
    {
      "globs": ["src/graphql/**/*.ts"],
      "output": "src/graphql/index.ts"
    }
  ],
  "gen-urls": [
    {
      "path": "src/storage/",
      "output": "src/common/fileurls.ts",
      "variable": "fileurls",
      "prefix": ""
    }
  ]
}
